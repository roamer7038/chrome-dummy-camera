{
  "manifest_version": 2,
  "name": "Dummy Camera",
  "version": "0.1.0",
  "description": "A dummy camera extension for Developers",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "js": [
        "loader.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "cs.js"
  ]
}
